{
  "expo": {
    "name": "QR Code Generator",
    "slug": "qrcodegen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/qrcode.png",
    "scheme": "qrcodegen",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/qrcode.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5267f322-c6e6-40f4-a6d3-e3fefee84211"
      }
    },
    "android": {
      "package": "com.babartech.qrcodegenerator"
    }
  }
}
